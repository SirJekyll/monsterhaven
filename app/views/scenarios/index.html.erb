<div class="card my-3 bg-dark text-light">
  <div class="card-body">
    <div class="card-title">
      <h1>
        Scenarios
      </h1>
    </div>
  </div>
</div>
<div class="list-group">
  <!-- <a href="#" class="list-group-item list-group-item-primary list-group-item-action">
    Create custom scenario
  </a> -->
  <% @scenarios.each do |scenario| %>
    <a href="<%= scenario_path(scenario) %>" class="list-group-item list-group-item-action bg-dark text-light">
      <%= "##{scenario.reference_number} #{scenario.name}" %>
    </a>
  <% end %>
</div>
