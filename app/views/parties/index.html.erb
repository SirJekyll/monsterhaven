<div class="card my-3 bg-dark text-light">
  <div class="card-body">
    <div class="card-title">
      <h1>
        Parties
      </h1>
    </div>
  </div>
</div>
<div class="list-group">
  <a href="<%= new_party_path %>" class="list-group-item list-group-item-action list-group-item-primary">
    Create New
  </a>
  <% @parties.each do |party| %>
    <a href="<%= edit_party_path(party) %>" class="list-group-item list-group-item-action bg-dark text-light">
      <%= party.name %>
    </a>
  <% end %>
</div>
